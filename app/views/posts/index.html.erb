<h1>All posts</h1>

<table>
  <tr>  
    <th>Title</th>
    <th>Text</th>
    <th>Author</th>
  </tr>
  <% @posts.each do |post| %>
   <tr>
    <td><%= post.title %></td>
    <td><%= post.text %></td>
    <td><%= post_author(post) %></td>
  </tr>
<%end%>
</table>

<br/><br/>

<%=link_to "New Post", new_post_path%>

<%= index_session %>
